package ch09.sec01.exam01;

/**바깥 클래스**/
	class A {
		A() {System.out.println("A객체가 생성됨");}
}

	/**인스턴스 멤버 클래스**/
	class B {
		B(){System.out.println("B객체가 생성됨");}
		int field;
		// static int field2;
		void method1() {}
		// static void method2(){}
	}
	
	/**정적 멤버 클래스**/
	static class C {
		C() {System.out.println("C객체가 생성됨");
		int field1;
		static int field2;
		void method1() {}
		static void method2() {}
		}
	}

